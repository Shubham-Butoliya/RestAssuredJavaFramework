{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "gender": {
      "type": "string",
      "enum": ["male", "female"]
    },
    "name": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "status": {
      "type": "string",
      "enum": ["active", "inactive"]
    }
  },
  "required": ["gender", "name", "id", "email", "status"]
}